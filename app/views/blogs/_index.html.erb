<div style="padding-left: 10px">
  <% @other_blogs.each do |blog| %>
    <h2>Blog</h2>
    <h4><%= blog.blog_title %></h4>
    <p style="padding-left: 20px"><%= blog.description %></p>
     <p id="likes-count">Likes : <%= pluralize(blog.likes.count, 'Like') %></p>
    <% previous_like = blog.likes.find { |like| like.user_id == @current_user.id} %>
<div id="like-button"><%= like_button_tag blog, previous_like %></div><br>
    <%= link_to pluralize(blog.comments.count, 'comment'), blog_comments_path(blog) %>

  <% end %>
</div>

